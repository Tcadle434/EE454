%out is a vector of key,value vectors
%key is index, value is original value
%just going to use insertion sort
function out = sourt(arr)
    sz = size(arr);
    out = double(zeros(sz(0),2));
    ones = ones(sz(0),2);
    out = out - (-1*ones);
    
    for i=1:sz
       index = 0;
       while(index <= sz && out(index,0) ~= -1 ...
           && arr(index) > out(index,1))
            
           index = index + 1;
       end
    end
    
end